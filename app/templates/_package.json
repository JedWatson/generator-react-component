{
  "name": "<%= packageName %>",
  "version": "1.0.0",
  "description": "<%= projectName %>",
  "main": "lib/<%= componentName %>.js",
  "author": "<%= developerName %>",
  "homepage": "https://github.com/<%= ghUser %>/<%= ghRepo %>",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= ghUser %>/<%= ghRepo %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= ghUser %>/<%= ghRepo %>/issues"
  },
  "dependencies": {
    "classnames": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "gulp": "^3.9.0",
    "react": "^15.0.1",
    "react-component-gulp-tasks": "^0.7.6",
    "react-dom": "^15.4.2"
  },
  "peerDependencies": {
    "react": "^15.0.1"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build",
    "examples": "gulp dev:server",
    "lint": "eslint ./; true",
    "publish:site": "NODE_ENV=production gulp publish:examples",
    "release": "NODE_ENV=production gulp release",
    "start": "gulp dev",
    "test": "echo \"no tests yet\" && exit 0",
    "watch": "gulp watch:lib"
  },
  "keywords": [
    "react",
    "react-component"
  ]
}

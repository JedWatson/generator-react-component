{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "description": "<%= projectName %>",
  "main": "src/<%= componentName %>.js",
  "author": "<%= developerName %>",
  "homepage": "https://github.com/<%= ghUser %>/<%= ghRepo %>",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= ghUser %>/<%= ghRepo %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= ghUser %>/<%= ghRepo %>/issues"
  },
  "dependencies": {
    "classnames": "^2.1.1",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "react-component-gulp-tasks": "^0.7.0",
    "gulp": "^3.8.11"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build",
    "examples": "gulp dev:server",
    "publish:site": "gulp publish:site",
    "release": "gulp release",
    "start": "gulp dev",
    "test": "echo \"no tests yet\" && exit 0",
    "watch": "gulp watch:lib"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "react",
    "react-component"
  ]
}
